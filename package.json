{
  "name": "repository-provider-test-support",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "test support for repository providers",
  "keywords": [
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "content-entry": "^4.1.2"
  },
  "devDependencies": {
    "ava": "^4.0.1",
    "c8": "^7.11.0",
    "documentation": "^13.2.5",
    "repository-provider": "^26.4.0",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=14.18.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/repository-provider-test-support.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/repository-provider-test-support/issues"
  },
  "homepage": "https://github.com/arlac77/repository-provider-test-support#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
