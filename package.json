{
  "name": "repository-provider-test-support",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./src/module.mjs",
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "test support for repository providers",
  "keywords": [
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/module.mjs"
  },
  "dependencies": {
    "content-entry": "^2.5.1"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.1.0",
    "repository-provider": "^23.13.3",
    "semantic-release": "^17.2.2"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/repository-provider-test-support.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/repository-provider-test-support/issues"
  },
  "homepage": "https://github.com/arlac77/repository-provider-test-support#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
